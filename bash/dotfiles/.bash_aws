
export AWS_SAM_LOCAL=1

# Atualiza o path para ser poss√≠vel acessar o comando aws, entre outros
if test -f ~/.local/bin/aws ; then
  chmod u+x ~/.local/bin/aws 
fi
export PATH=$PATH:$HOME/.composer/vendor/bin:$HOME/.local/bin
