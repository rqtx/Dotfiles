# CrunchyData config

# Configurando as pastas do GO e CrunchyData
export GOPATH=$HOME/go
export GOBIN=$GOPATH/bin
export PATH=$PATH:$GOBIN
export CO_NAMESPACE=operators
export CO_CMD=kubectl
export COROOT=$GOPATH/src/github.com/crunchydata/postgres-operator
export CO_IMAGE_PREFIX=gcr.io/ajudici-prod/aj/postgres
export CO_BASEOS=centos7-10.3
export CO_VERSION=1.0.1
export CO_IMAGE_TAG=$CO_BASEOS-$CO_VERSION

source $GOBIN/pgo-bash-completion

# for the pgo CLI auth
export PGO_CA_CERT=$COROOT/conf/postgres-operator/server.crt
export PGO_CLIENT_CERT=$COROOT/conf/postgres-operator/server.crt
export PGO_CLIENT_KEY=$COROOT/conf/postgres-operator/server.key
export PGO_NAMESPACE=operators
export PGO_CMD=kubectl
